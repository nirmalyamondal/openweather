{
    "name": "ashokatree/openweather",
    "type": "typo3-cms-extension",
    "description": "Current and Forecast weather data by OpenWeather.",
    "version": "10.0.0",
    "keywords": [
        "ashokatree",
        "openweather",
        "weather",
        "api weather"
    ],
    "homepage": "https://ashokatree.net",
    "authors": [
        {
            "name": "Nirmalya Mondal",
            "email": "nirmalya.mondal@gmail.com",
            "role": "Developer"
        }
    ],
    "license": [
        "GPL-2.0-or-later"
    ],
    "minimum-stability": "stable",
    "require": {
        "typo3/cms-core": "^10.0.0",
        "typo3/cms-scheduler": "^10.0.0"
    },
	"extra": {
		"typo3/cms": {
			"extension-key": "openweather"
		}
	},
    "autoload": {
        "psr-4": {
            "AshokaTree\\Openweather\\": "Classes"
        }
    }
